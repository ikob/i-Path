#
# DEFAULTS -- Default kernel configuration file for FreeBSD/ia64
#
# $FreeBSD: src/sys/ia64/conf/DEFAULTS,v 1.11.6.1 2008/11/25 02:59:29 kensmith Exp $

machine		ia64

# Bus support.
device 		acpi		# ACPI support

# Pseudo devices.
device		mem		# Memory and kernel memory devices

# UART chips on this platform
device		uart_ns8250

options 	GEOM_BSD
options 	GEOM_PART_GPT
options 	GEOM_PART_MBR

# KSE support went from being default to a kernel option
options 	KSE
